@model IEnumerable<GummyBearsForever.Models.Inventory>
@{
    ViewData["Title"] = "Gummy Bears Forever";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h3>Current Inventory</h3>
<table class="table>"
@foreach (var item in Model)
{
    <tr>
        <td>
            @Html.ActionLink(item.GummyName, "Details", new { id = item.GummyId }) 
        </td>
    </tr>

}
<hr />
    </table>
@Html.ActionLink("Add a new gummy", "Create")
