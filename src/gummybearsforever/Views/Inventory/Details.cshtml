@model GummyBearsForever.Models.Inventory
@{
    ViewData["Title"] = "Gummy Bears Forever";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h2>Details</h2>
<hr />
@Html.ActionLink("Add New", "Create")
<div class="container"
<div class="row">
    <div class="col s12 m12">
        <div class="card blue-grey darken-1">
            <div class="card-content white-text">
                <span class="card-title">@Html.DisplayNameFor(model => model.GummyName): @Html.DisplayFor(model => model.GummyName)</span>
                <p>
                <p>@Html.DisplayNameFor(model => model.GummyName): @Html.DisplayFor(model => model.GummyName)</p>
                <p>@Html.DisplayNameFor(model => model.Cost): @Html.DisplayFor(model => model.Cost)</p>
                <p>@Html.DisplayNameFor(model => model.Country): @Html.DisplayFor(model => model.Country)</p>
                </p>
            </div>
            <div class="card-action">
                @Html.ActionLink("Update", "Update", new { id = Model.GummyId })
                @Html.ActionLink("Delete", "Delete", new { id = Model.GummyId })
            </div>
        </div>
    </div>
</div>
    </div>



